{
    "name": "brianvarskonst/nikas",
    "type": "wordpress-theme",
    "description": "TODO: Describe what this package is all about",
    "license": "MIT",
    "authors": [
        {
            "name": "Brianvarskonst",
            "email": "brianvarskonst@example.com",
